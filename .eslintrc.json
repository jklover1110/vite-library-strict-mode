{
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "plugins": ["prettier"],
  "root": true,
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  }
}
